<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuAlumnos" android:background="#00BCD4">

    <ImageView
            android:layout_width="153dp"
            android:layout_height="221dp" app:srcCompat="@mipmap/asas" android:id="@+id/imageView6"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="242dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="138dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"/>
    <TextView
            android:text="MATRICULA DE CURSOS"
            android:layout_width="374dp"
            android:layout_height="164dp" android:id="@+id/textView10"
